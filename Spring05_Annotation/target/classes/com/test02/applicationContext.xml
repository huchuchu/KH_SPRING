<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">

	<!-- ~~를 읽어서 주입해준다 -->
	<context:annotation-config/>
	


	<bean id="student1" class="com.test02.Student">
		<property name="name" value="유슬기"/>
		<property name="age" value="31"/>
		<property name="addr" value="경기 용인"/>
	</bean>
	
	<bean id="student2" class="com.test02.Student">
		<property name="name" value="hoho"></property>
		<property name="age" value="33"></property>
		<property name="addr" value="에러에러"></property>
	</bean>
	
	
	<bean id="school" class="com.test02.School">
		<property name="grade" value="1"/>	
	</bean>
	
	
</beans>
